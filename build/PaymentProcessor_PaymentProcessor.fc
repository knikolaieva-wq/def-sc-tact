        $PaymentProcessor$_contract_store(($self'owner, $self'platformCommissionBps, $self'mainCommissionWallet, $self'nftCollection, $self'nonces));
        return ();
    }
    
    ;; Receive Transfer message
    if (op == 1120014202) {
        var ($t'req'buyer, $t'req'seller, $t'req'amount, $t'req'buyerPaysCommission, $t'req'optionalCommissionWallet) = in_msg~$PaymentRequest$_load();
        slice sc_1 = in_msg~load_ref().begin_parse();

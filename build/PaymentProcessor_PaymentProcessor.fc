        var $m'newBps = in_msg~load_int(257);
        throw_unless(58772, ( equal_slices_bits(__tact_context_get_sender(), $self'owner) ));
        throw_unless(57790, ( (($m'newBps >= 0)) ? (($m'newBps <= 10000)) : (false) ));
        $self'platformCommissionBps = $m'newBps;

        var ($req'buyer, $req'seller, $req'amount, $req'buyerPaysCommission, $req'optionalCommissionWallet) = ($t'req'buyer, $t'req'seller, $t'req'amount, $t'req'buyerPaysCommission, $t'req'optionalCommissionWallet);
        throw_unless(62794, ( ~ equal_slices_bits($req'seller, $req'buyer) ));
        throw_unless(54178, ($req'amount > 0));
        throw_unless(15534, ($global_now() <= $t'deadline));
        int $currentNonce = __tact_dict_get_slice_int($self'nonces, 267, $req'buyer, 257);
        throw_unless(42041, ( equal_slices_bits(__tact_context_get_sender(), $req'buyer) ));
        int $nextNonce = ((~ null?($currentNonce)) ? ($currentNonce + 1) : 1);
        $self'nonces~__tact_dict_set_slice_int(267, $req'buyer, $nextNonce, 257);
        int $commission = (($req'amount * $self'platformCommissionBps) / 10000);
        int $totalPay = ($req'buyerPaysCommission ? ($req'amount + $commission) : $req'amount);
        throw_unless(47884, ($Context$_get_value(__tact_context_get()) >= $totalPay));
        if ($req'buyerPaysCommission) {
            $global_send($SendParameters$_constructor_to_value($req'seller, $req'amount));
        } else {
            $global_send($SendParameters$_constructor_to_value($req'seller, ($req'amount - $commission)));
        }
        int $share = 0;
        int $optionalPart = (($share > 0) ? (($commission * $share) / 10000) : 0);
        int $platformPart = ($commission - $optionalPart);
        if (($platformPart > 0)) {
            $global_send($SendParameters$_constructor_to_value($self'mainCommissionWallet, $platformPart));
        }
        if (( (($optionalPart > 0)) ? ((~ null?($req'optionalCommissionWallet))) : (false) )) {
            $global_send($SendParameters$_constructor_to_value($req'optionalCommissionWallet, $optionalPart));
        }
